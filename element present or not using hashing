#include <iostream>
#include<cmath>
using namespace std;
#define MAX 1000

bool hsh[MAX+1][2];
bool search(int x){
	if(x>=0){
		if(hsh[x][0]==1){
			return true;
		}
		else
			return false;
		
	}
	else{
			x=abs(x);
			if(hsh[x][1]==1){
				return true;
			}
			else{
				return false;
			}
		}
}
void insert(int arr[], int n){
	for(int i=0; i<n; i++){
		if(arr[i]>=0){
			hsh[arr[i]][0]=1;
		}
		else{
			hsh[abs(arr[i])][1]=1;
		}
	}
}
int main() { 
	int arr[]={-1, 9,4,-2,12};
	int n = sizeof(arr)/sizeof(arr[0]);
	insert(arr,n);
	int find = -3;
	if(search(find)){
		cout<<"present"<<endl;
	}
	else{
		cout<<"not present"<<endl;
	}
    
}
